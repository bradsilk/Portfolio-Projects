   "source": [
    "# 1. Take the first and last purchase of each customer\n",
    "\n",
    "first_purchase = df_orders.sort_values(by='OrderDate').groupby('_CustomerID').first()\n",
    "last_purchase = df_orders.sort_values(by='OrderDate').groupby('_CustomerID').last()\n",
    "\n",
    "# 2. Merge first_purchase and last_purchase into a single dataframe\n",
    "customer = pd.merge(first_purchase, last_purchase,on='_CustomerID')[['OrderDate_x','OrderDate_y']]\n",
    "customer.rename({'OrderDate_x':'First_Purchase','OrderDate_y':'Last_Purchase'},axis=1,inplace=True)\n",
    "customer = customer.reset_index()\n",
    "customer = pd.merge(customer,df_cust,on='_CustomerID')\n",
    "customer = customer[customer['First_Purchase'] <= '2019-01-01']\n",
    "customer"
   ]
  },
