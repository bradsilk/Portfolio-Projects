   "source": [
    "### 4.) Analysis + Insights"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 446,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Data prep + feature engineering:\n",
    "\n",
    "#Additional columns needed: Profit (Unit Price - Unit Cost)\n",
    "    #*Assumes Unit Price is the sale price, Unit Cost is the manufacturing cost, and the discount has already been applied to Unit Price\n",
    "        #This was not specified in the data source, data.world/dataman-udit/us-regional-sales-data\n",
    "        \n",
    "df_orders['Profit'] =  df_orders['Unit Price'] - df_orders['Unit Cost']\n",
    "\n",
    "#Grouped view:\n",
    "    #Grouped by City Name and State because the same city name can appear for multiple states\n",
    "city_group = pd.merge(df_orders, df_loc, how='inner', on ='_StoreID').groupby(['City Name','State'])\n",
    "\n",
    "#Sales:\n",
    "    #Average:\n",
    "avg_sales=city_group['Unit Price'].mean().sort_values(ascending=False).reset_index()\n",
    "    #Sum:\n",
    "sum_sales=city_group['Unit Price'].sum().sort_values(ascending=False).reset_index()\n",
    "\n",
    "    \n",
    "#Profit:\n",
    "    #Average:\n",
    "avg_profit=city_group['Profit'].mean().sort_values(ascending=False).reset_index()\n",
    "    #Sum:\n",
    "sum_profit=city_group['Profit'].sum().sort_values(ascending=False).reset_index()\n",
    "    \n",
    "#Discount:\n",
    "    #Average\n",
    "avg_discount=city_group['Discount Applied'].mean().sort_values(ascending=False).reset_index()\n",
    "    #Sum:\n",
    "sum_discount=city_group['Discount Applied'].sum().sort_values(ascending=False).reset_index()\n",
    "\n",
    "\n",
    "#Categorical Labels:\n",
    "    #High/Low Average Sales:\n",
    "high_avg_sales = avg_sales[avg_sales['Unit Price'] > \\\n",
    "                                  np.percentile(avg_sales['Unit Price'].tolist(),75)] \n",
    "low_avg_sales = avg_sales[avg_sales['Unit Price'] <= \\\n",
    "                                  np.percentile(avg_sales['Unit Price'].tolist(),25)]\n",
    "\n",
    "    #High/Low Average Profit:\n",
    "high_avg_profit = avg_profit[avg_profit['Profit'] > \\\n",
    "                                  np.percentile(avg_profit['Profit'].tolist(),75)] \n",
    "low_avg_profit = avg_profit[avg_profit['Profit'] <= \\\n",
    "                                  np.percentile(avg_profit['Profit'].tolist(),25)]\n",
    "\n",
    "    \n",
    "    #High/Low Average Discount:\n",
    "high_avg_discount = avg_discount[avg_discount['Discount Applied'] > \\\n",
    "                                  np.percentile(avg_discount['Discount Applied'].tolist(),75)]\n",
    "low_avg_discount = avg_discount[avg_discount['Discount Applied'] <= \\\n",
    "                                  np.percentile(avg_discount['Discount Applied'].tolist(),25)]\n",
    "\n",
    "\n",
    "#Isolating month and year from OrderDate:\n",
    "\n",
    "df_orders['month_year'] = df_orders['OrderDate'].apply(lambda x: x.strftime('%Y-%m'))"
   ]
  },
