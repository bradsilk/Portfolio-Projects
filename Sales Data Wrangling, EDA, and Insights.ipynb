    "df_select1 = pd.merge(df_select1, df_team, how='inner', on ='_SalesTeamID')\n",
    "df_select1 = pd.merge(df_select1, df_prod, how='inner', on ='_ProductID')\n",
    "\n",
    "# 3. Obtain columns of interest from resulting df and group by region, product name\n",
    "df_select1 = df_select1[['OrderNumber','Region','Product Name']]\n",
