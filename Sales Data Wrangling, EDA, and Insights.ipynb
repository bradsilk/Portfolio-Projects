   "source": [
    "# 3. Create additional features (retention days, order count, order frequency, average revenue/order)\n",
    "\n",
    "customer['Retention_Days'] = customer['Last_Purchase'] - customer['First_Purchase']\n",
    "customer['Retention_Days'] = customer['Retention_Days'].dt.days\n",
    "customer['Count_of_Orders'] = df_orders.groupby('_CustomerID')['Unit Price'].count()\n",
    "customer['Freq_of_Orders'] = round(customer['Retention_Days']/customer['Count_of_Orders'],0)\n",
    "customer['Avg_Revenue_per_order'] = df_orders.groupby('_CustomerID')['Unit Price'].mean()\n",
    "\n",
    "\n",
    "#Top 3 most loyal customers:\n",
    "\n",
    "top_customers = customer[(customer['Retention_Days'] >= 730) & (customer['Freq_of_Orders'] <= 30 ) & (customer['Avg_Revenue_per_order'] > 2250)]\n",
    "\n",
    "top_customers.sort_values(by='Retention_Days',ascending=False).head(3)['Customer Names']\n"
   ]
  },
