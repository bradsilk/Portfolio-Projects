    "df_select1.rename(columns={'OrderNumber':'Order Count'}, inplace=True)\n",
    "\n",
    "\n",
    "# 4. Sort products by their order count in descending order\n",
    "df_select1 = df_select1.groupby(['Region']).apply(lambda x: x.sort_values(['Order Count'],ascending = False)).reset_index(drop = True)\n",
    "\n",
    "# 5. Select top 5 items by order count for each region\n",
    "df_prod_x_reg = df_select1.groupby(['Region']).head()\n",
